{
  "allow_data_on_tables": ["admins", "invoice_items", "invoices", "organizations", "transactions" ],
  "filter_inserts": {
    "admins": [
      "cel:is_super==1"
    ],
    "service_orders": [
      "cel:processed==0"
    ],
    "invoices": [
      "status!=10",
      "status!=20",
      "status!=30",
      "status!=40"
    ],
    "invoice_items": [
      "purpose==1",
      "organization_id->invoices.organization_id",
      "organization_id->organizations.id"
    ],
    "transactions": [
      "organization_id->invoices.organization_id",
      "organization_id->organizations.id"
    ]
  }
}
