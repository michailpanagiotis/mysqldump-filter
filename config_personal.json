{
  "allow_data_on_tables": ["admins", "invoice_items", "invoices", "organizations", "transactions" ],
  "cascades": {
    "invoice_items": [
      "invoice_id->invoices.id"
    ],
    "transactions": [
      "invoice_id->invoices.id"
    ]
  },
  "filter_inserts": {
    "admins": [
      "cel:is_super==1"
    ],
    "service_orders": [
      "cel:processed==0"
    ],
    "invoices": [
      "cel:owner_id==7",
      "cel:date>timestamp(\"2025-01-01\")"
    ],
    "invoice_items": [
      "cel:user_id==7"
    ]
  }
}
